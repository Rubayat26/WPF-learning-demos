<Window x:Class="CalendarBlockedOutDates.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CalendarBlockedOutDates"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Viewbox>
        <Calendar Name="cldSample" SelectionMode="MultipleRange">
            <Calendar.BlackoutDates>
                <CalendarDateRange Start="10.13.2022" End="10.19.2022" />
                <CalendarDateRange Start="10.27.2022" End="10.31.2022" />
            </Calendar.BlackoutDates>
        </Calendar>
    </Viewbox>
</Window>
